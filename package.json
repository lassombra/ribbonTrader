{
  "name": "RibbonTrader",
  "version": "0.0.1",
  "description": "Ribbon Trader app",
  "main": "main.js",
  "scripts": {
    "start": "npm-run-all build server",
    "build": "npm-run-all --parallel build:client build:server",
    "build:watch": "npm-run-all --parallel build:client:watch build:server:watch",
    "build:client": "webpack --config webpack.client.config.js",
    "build:client:watch": "webpack --watch --config webpack.client.config.js",
    "build:server": "webpack --config webpack.config.js",
    "build:server:watch": "webpack --watch --config webpack.config.js",
    "clean": "rimraf .build/.client && rimraf .build/.server",
    "server": "cd .build && node .server/app.bundle.js && cd .."
  },
  "author": "",
  "license": "UNLICENSED",
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-class-properties": "^6.22.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-function-bind": "^6.22.0",
    "babel-plugin-transform-react-jsx": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-stage-3": "^6.22.0",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "faker": "^3.1.0",
    "file-loader": "^0.10.0",
    "java-properties-loader": "^0.2.0",
    "json-loader": "^0.5.4",
    "mocha": "^3.2.0",
    "mocha-circleci-reporter": "0.0.2",
    "node-sass": "^4.5.0",
    "node-style-loader": "0.0.1-alpha",
    "noop-loader": "^1.0.0",
    "npm-run-all": "^4.0.1",
    "null-loader": "^0.1.1",
    "rimraf": "^2.5.4",
    "sass-loader": "^5.0.0",
    "style-loader": "^0.13.1",
    "typescript-loader": "^1.1.3",
    "webpack": "^2.2.1",
    "webpack-manifest-plugin": "^1.1.0"
  },
  "dependencies": {
    "apollo-client": "^0.8.4",
    "babel-runtime": "^6.22.0",
    "body-parser": "^1.16.0",
    "bootstrap-sass": "^3.3.7",
    "express": "^4.14.1",
    "graphql": "^0.9.1",
    "graphql-server-express": "^0.6.0",
    "graphql-tag": "^1.2.4",
    "graphql-tools": "^0.10.0",
    "isomorphic-fetch": "^2.2.1",
    "moment": "^2.17.1",
    "path-to-regexp": "^1.7.0",
    "qs": "^6.3.0",
    "react": "^15.4.2",
    "react-apollo": "^0.10.0",
    "react-dom": "^15.4.2",
    "react-helmet": "^4.0.0",
    "source-map-support": "^0.4.11",
    "transducers.js": "^0.3.2"
  }
}
